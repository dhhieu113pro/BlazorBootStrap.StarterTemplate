{
  "$schema": "http://json.schemastore.org/template",
  "author": "Hieu Dam",
  "classifications": [
    "Web",
    "Blazor",
    "WebAssembly",
    "Bootstrap"
  ],
  "identity": "BlazorBootStrap.StarterTemplate",
  "name": "Blazor auto global mode with Bootstrap Starter",
  "shortName": "blazor-bootstrap-auto-global",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "BlazorBootStrap",
  "preferNameDirectory": true,
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net10.0",
          "description": "Target .NET 10"
        },
        {
          "choice": "net9.0",
          "description": "Target .NET 9"
        },
        {
          "choice": "net8.0",
          "description": "Target .NET 8"
        }
      ],
      "defaultValue": "net10.0",
      "replaces": "net10.0"
    },
    "HostIdentifier": {
      "type": "bind",
      "binding": "HostIdentifier"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "**/[Bb]in/**",
            "**/[Oo]bj/**",
            ".template.config/**/*",
            "**/*.filelist",
            "**/*.user",
            "**/*.lock.json",
            "**/.vs/**",
            "**/.vscode/**",
            "**/.idea/**",
            "**/node_modules/**"
          ]
        }
      ]
    }
  ]
}
